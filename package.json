  },
  "homepage": "https://github.com/aswaddev/pakistan-covid19#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "express": "^4.17.3",
    "moment": "^2.24.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
